@model ProjectDotNetV2.Data.Entities.Order
@{
    ViewData["Title"] = "Details";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
<div id="page-wrapper">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Product</h1>
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- /.row -->
        <div class="row">
            <div class="col-lg-12">
                <div class="panel panel-default">
                    <!-- /.panel-heading -->
                    <div class="panel-body">
                        <div class="table-responsive">


                            <div class="row">
                                <form asp-action="Create">

                                    <div class="col-md-6">
                                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                                        <div class="form-group">
                                            <label asp-for="Name" title="" class="control-label"> @Html.DisplayNameFor(model => model.FirstName)</label>
                                            <input asp-for="Name" class="form-control" value=" @Html.DisplayFor(model => model.FirstName)" disabled="disabled" />
                                            <span asp-validation-for="Name" class="text-danger"></span>
                                        </div>
                                        <div class="form-group">
                                            <label asp-for="Name" title="" class="control-label"> @Html.DisplayNameFor(model => model.StreetAddress)</label>
                                            <input asp-for="Name" class="form-control" value=" @Html.DisplayFor(model => model.StreetAddress)" disabled="disabled" />
                                            <span asp-validation-for="Name" class="text-danger"></span>
                                        </div>
                                        <div class="form-group">
                                            <label asp-for="Color" class="control-label">
                                                @Html.DisplayNameFor(model => model.Country)
                                            </label>
                                            <input asp-for="Color" class="form-control" value="@Html.DisplayFor(model => model.Country)" disabled="disabled" />
                                            <span asp-validation-for="Color" class="text-danger"></span>
                                        </div>
                                        <div class="form-group">
                                            <label asp-for="Price" class="control-label">
                                                @Html.DisplayNameFor(model => model.PhoneNumber)
                                            </label>
                                            <input asp-for="Price" class="form-control" value="@Html.DisplayFor(model => model.PhoneNumber)" disabled="disabled" />
                                            <span asp-validation-for="Price" class="text-danger"></span>
                                        </div>
                                        <div class="form-group">
                                            <label asp-for="Price" class="control-label">
                                                @Html.DisplayNameFor(model => model.OrderDetails)
                                            </label>
                                            <input asp-for="Price" class="form-control" value="@Html.DisplayFor(model => model.OrderDetails)" disabled="disabled" />
                                            <span asp-validation-for="Price" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="Quantity" class="control-label">
                                                @Html.DisplayNameFor(model => model.LastName)
                                            </label>
                                            <input asp-for="Quantity" class="form-control" value="@Html.DisplayFor(model => model.LastName)" disabled="disabled" />
                                            <span asp-validation-for="Quantity" class="text-danger"></span>
                                        </div>
                                        <div class="form-group">
                                            <label asp-for="Quantity" class="control-label">
                                                @Html.DisplayNameFor(model => model.City)
                                            </label>
                                            <input asp-for="Quantity" class="form-control" value="@Html.DisplayFor(model => model.City)" disabled="disabled" />
                                            <span asp-validation-for="Quantity" class="text-danger"></span>
                                        </div>


                                        <div class="form-group">
                                            <label asp-for="Gender" class="control-label">
                                                @Html.DisplayNameFor(model => model.OrderTotal)
                                            </label>
                                            <input asp-for="Gender" class="form-control" value="@Html.DisplayFor(model => model.OrderTotal)" disabled="disabled" />
                                            <span asp-validation-for="Gender" class="text-danger"></span>
                                        </div>
                                        <div class="form-group">
                                            <label asp-for="Gender" class="control-label">
                                                @Html.DisplayNameFor(model => model.OrderPlaced)
                                            </label>

                                            <input asp-for="Gender" class="form-control" value="@Html.DisplayFor(model => model.OrderPlaced)" disabled="disabled" />
                                            <span asp-validation-for="Gender" class="text-danger"></span>
                                        </div>
                                        <div class="form-group">
                                            <label asp-for="Gender" class="control-label">
                                                @Html.DisplayNameFor(model => model.Email)
                                            </label>
                                            <input asp-for="Email" class="form-control" value="@Html.DisplayFor(model => model.Email)" disabled="disabled"/>
                                            <span asp-validation-for="Gender" class="text-danger"></span>
                                        </div>
                                    </div>


                                </form>
                          
                            </div>
                        </div>



                        <!-- /.table-responsive -->
                    </div>
                    <!-- /.panel-body -->
                </div>
                <!-- /.panel -->
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- /.row -->
        <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
</div>




